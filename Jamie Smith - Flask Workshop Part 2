numbers')\n","def randomNumbers():\n","    randNum = random.randint(0,9)\n","    randNum *= 2\n","    randNum -= 2\n","    if (randNum >= 10):\n","        return str(randNum)\n","\n","#--------------------------HTMLEXAMPLE\n","@app.route('/htmlexample')\n","def htmlexample():\n","    title = \"Your number: \"\n","    randNum = random.randint(0,9)\n","    randNum = str(randNum)\n","    return '''\n","    <html>\n","        <head>\n","            <title>Home Page</title>\n","        </head>\n","        <body>\n","            <link href=\"https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&display=swap\" rel=\"stylesheet\">\n","            <div style=\"font-family: 'Montserrat', sans-serif;background:linear-gradient(222deg, #f45727, #ff105a, #ffa300);padding:15px;margin:15px;color:#ffffff;border-radius:10px;\">\n","            <h1>''' + title + randNum + '''</h1>\n","            <h3>life is pain u either kill or be killed</h3>\n","            <p>watchu gon do lorem ipsum</p>\n","            <hr style=\"border-top: 6px dotted white;border-bottom:none;\">\n","            <br>\n","            <table><tr><td>\n","            <img src=\"https://pbs.twimg.com/media/C5ep25NWcAES63D.jpg\" style=\"border-radius:50%;width:100px;height:100px;\">\n","            </td><td style=\"color:white;\"><code>square up</code><br><code>square up</code><br><code>square up</code></td></tr></table>\n","            </div>\n","        </body>\n","    </html>'''\n","\n","#--------------------------JSONEXAMPLE\n","@app.route('/jsonexample')\n","def jsonexample():\n","    return{'data' : {'test : 5'}}\n","\n","#--------------------------GETTOTAL\n","@app.route('/gettotal')\n","def gettotal():\n","    total = float(request.args['total'])\n","    tip = \"The tip is \" + str(total * 0.2)\n","    combined = \"The total is \" + str(total * 1.2)\n","    total = \"Your bill is \" + str(total)\n","    return '''\n","    <html>\n","    <h1>Tip Calculator</h1>\n","    <h3>''' + total + '''</h3>\n